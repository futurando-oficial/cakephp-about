{
    "name": "futurando-oficial/cakephp-about",
    "description": "About plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "futurando-oficial/cakephp-admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "About\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "About\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
